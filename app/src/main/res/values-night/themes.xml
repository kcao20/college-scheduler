<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CollegeScheduler" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="android:textColorPrimary">@color/rosé_pine_text</item>
        <item name="colorPrimary">@color/rosé_pine_overlay</item>
        <item name="colorPrimaryVariant">@color/rosé_pine_subtle</item>
        <item name="colorOnPrimary">@color/rosé_pine_text</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/rosé_pine_iris</item>
        <item name="colorSecondaryVariant">@color/rosé_pine_foam</item>
        <item name="colorOnSecondary">@color/rosé_pine_text</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorSecondary</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/rosé_pine_base</item>
    </style>
</resources>